{
  "name": "abdfal/my-php-library",
  "license": "proprietary",
  "description": "My PHP Library for Tax Calculation",
  "type": "library",
  "authors": [
    {
      "name": "abdfal",
      "email": "naufaldec07@gmail.com",
      "homepage": "https://github.com/abdfal"
    }
  ],
  "require": {
    "monolog/monolog": "1.23.*",
    "php": "^8.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.org"
    }
  ],
  "provide": {
    "ext-json": "1.3.0"
  },
  "suggest": {
    "ext-mbstring": "For better Unicode support"
  },
  "extra": {
    "my-library": {
      "config-file": "config.php"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "classmap-authoritative": true,
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "App\\Calculate\\": "App/Calculate/"
    }
  },
  "minimum-stability": "beta",
  "homepage": "https://github.com/abdfal/Tax-Calculation",
  "support": {
    "email": "naufaldec07@gmail.com",
    "issues": "https://github.com/abdfal/Tax-Calculation/issues",
    "source": "https://github.com/abdfal/Tax-Calculation"
  }
}
